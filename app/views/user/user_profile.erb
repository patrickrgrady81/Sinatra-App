<h1><%= current_user.user_name %></h1>

<p>You have <%= @recipes.count %> saved recipe(s)</p>
<% @recipes.each do |recipe| %>
<div class="card">
  <div class="card-header">
    <%= recipe.name %>
  </div>
  <div class="card-body">
    <h5 class="card-title"><%= recipe.description %></h5>
    <a href="#" class="btn btn-primary">Go somewhere</a>
  </div>
</div>
  
<% end %>
